Description='RUWireless_Secure network (custom configuration)'
Interface=wlo1
Connection=wireless
Security=wpa-configsection
IP=dhcp
WPAConfigSection=(
'ssid="RUWireless_Secure"'
'priority=1'
'key_mgmt=WPA-EAP'
'eap=TTLS'
'proto=WPA2'
'phase2="auth=PAP"'
'ca_cert="/usr/share/ca-certificates/trust-source/mozilla.trust.crt"'
'identity="INSERTEMAIL"'
'password="INSERTPASSWORD"' )
